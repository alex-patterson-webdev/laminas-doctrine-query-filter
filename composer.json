{
  "name": "alex-patterson-webdev/laminas-doctrine-query-filter",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Alex Patterson",
      "email": "alex.patterson.webdev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4 || >=8.0",
    "alex-patterson-webdev/laminas-factory": "^2",
    "doctrine/orm": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "Arp\\LaminasDoctrine\\Query\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArpTest\\LaminasDoctrine\\Query\\": "test/unit/"
    }
  },
  "scripts": {
    "test": "php vendor/bin/phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
